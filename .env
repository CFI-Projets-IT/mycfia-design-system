# Configuration par défaut (versionnée dans Git)
# Remplacée par .env.local pour les spécificités locales

# === SYMFONY ===
APP_ENV=dev
APP_SECRET=change-me-in-production-env-local

# === BASE DE DONNÉES ===
DB_ROOT_PASSWORD=root
DB_NAME=app_db
DB_USER=app_user
DB_PASSWORD=app_password
MARIADB_VERSION=11

# === DOCKER ===
PROJECT_NAME=myCfia
BUILD_TARGET=development

# === PORTS (modifiables en local) ===
HTTP_PORT=82
PHPMYADMIN_PORT=8082
MAILHOG_PORT=8027
MERCURE_PORT=3001

# === MERCURE ===
MERCURE_JWT_SECRET=!ChangeThisMercureHubJWTSecretKey!
MERCURE_VERSION=v0.16

# === VERSIONS OUTILS DÉVELOPPEMENT ===
PHPMYADMIN_VERSION=latest
MAILHOG_VERSION=latest